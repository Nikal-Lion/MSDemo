{
  "ConnectionStrings": {
    "MSDbContext": "server=172.31.2.111;database=msdb;user=ls;password=ls#it*123;"
  },
  "SiteSetting": {
    "WorkerId": 1, //for snowflake workerid
    "DataCenterId": 1, //for snowflake datacenterid
    "DefaultLanguage": "zh-Hans", //zh-Hans zh-Hant en
    "LoginFailedCountLimits": 3, //the number of login failed 
    "LoginLockedTimeout": 3 //(minutes) account locked timeout
  },
  "Startup": {
    "Cors": {
      "AllowOrigins": [
        "http://localhost:8080",
        "http://localhost:8081"
      ]
    }
  },
  "JwtSetting": {
    "Issuer": "MS.WebHost",
    "Audience": "MS.Audience",
    "SecurityKey": "MS.WebHost SecurityKey", //more than 16 chars
    "LifeTime": 1440 //(minutes) token life time default:1440 m=1 day
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  }
}
